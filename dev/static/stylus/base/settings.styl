*, *:before, *:after
	vertical-align baseline
	font-weight inherit
	font-family inherit
	font-style inherit
	font-size 100%
	border 0 none
	outline 0 none
	padding 0
	margin 0
	box-sizing border-box

html,body
	width 100%
	height 100%
	font-family: 'Montserrat', sans-serif;
	font-style: normal;
	font-weight: normal;
	font-size: 16px;
	line-height: 20px;
	color: #3E3E3E;

ul,li
	list-style-type none

p
	margin 0

a
	text-decoration none
	color black

img
	max-width 100%

audio,
canvas,
iframe,
img,
svg,
video
	vertical-align middle


textarea
	resize none

.fa-icon {
	font-family: 'Font Awesome 5 Pro';	
}
.text_container span {
    display: block;
}
.sect_title {
    font-weight: 600;
    font-size: 42px;
    line-height: 55px;
}
.sect_desc {
    font-size: 18px;
    line-height: 22px;
    letter-spacing: 0.02em;
    color: #3E3E3E;
    opacity: 0.9;
}
.btn {
    padding: 34px;
    min-width: 445px;
    font-weight: bold;
    font-size: 18px;
    line-height: 150%;
/* identical to box height, or 27px */
    font-family: 'Montserrat',sans-serif;
    letter-spacing: 0.05em;
    text-transform: uppercase;
    color: #FFFFFF;
    background: #26A4CC;
    box-shadow: 0px 41px 68px rgba(38, 164, 204, 0.2), inset 0px 7px 28px rgba(255, 255, 255, 0.1);
    border-radius: 5px;
}
.sect_margin {
	margin-top: 122px	
}
body.no-scroll {
	overflow hidden
}


